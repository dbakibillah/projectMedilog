        <TextField fx:id="TF_age" layoutX="85.0" layoutY="425.0" prefHeight="46.0" prefWidth="350.0" promptText="Age" style="-fx-background-color: transparent; -fx-border-color: #0598ff; -fx-border-width: 0px 0px 2px 0px;">
            <font>
                <Font size="17.0" />
            </font>
        </TextField>
        <Circle fx:id="ImageCIrcle" accessibleRole="IMAGE_VIEW" fill="DODGERBLUE" layoutX="157.0" layoutY="163.0" radius="79.0" stroke="BLACK" strokeType="INSIDE" />
        <Button fx:id="BTN_ChooseFile" layoutX="65.0" layoutY="286.0" mnemonicParsing="false" onAction="#onCLickBTN_ChooseFile" prefHeight="30.0" prefWidth="80.0" style="-fx-background-color: #0080FF;" text="Choose File" textFill="WHITE" />
        <Text fill="#908a8a" layoutX="88.0" layoutY="264.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Change/Upload photo">
